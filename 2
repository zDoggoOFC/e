--Coasting UI Lib by : https://v3rmillion.net/member.php?action=profile&uid=1393756
--Release thread here : https://v3rmillion.net/showthread.php?tid=1071849&pid=7590441

local CoastingLibrary = loadstring(game:HttpGet("https://pastebin.com/raw/3gQQtaKX"))()

local AimbotTab = CoastingLibrary:CreateTab("Scripts")
local MainSection = AimbotTab:CreateSection("Blox Fruits")
local Credits = AimbotTab:CreateSection("Have a great day!")

MainSection:CreateButton("MARKxHUB", function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/ArceusXHub/AV2022/main/README.md", true))()
end)

MainSection:CreateButton("Ren Hub", function()
   loadstring(game:HttpGet('https://raw.githubusercontent.com/SHAREHACK/bloxfruit/main/free'))()
end)

MainSection:CreateButton("Mukuro Hub", function()
   loadstring(game:HttpGet"https://raw.githubusercontent.com/xQuartyx/DonateMe/main/ScriptLoader")()
end)

Credits:CreateButton(":)", function()
   print(";)")
end)

Credits:CreateButton(":D", function()
   print(";D")
end)

Credits:CreateButton(":)", function()
   print(";)")
end)
