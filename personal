local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Personal Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroText = "Personal Hub"})

OrionLib:MakeNotification({
	Name = "Password System",
	Content = "Welcome to the Password System!",
	Image = "rbxassetid://4483345998",
	Time = 3
})

_G.Key = "2523"
_G.KeyInput = "string"

function MakeScriptHub()
	local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Script Personal", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Scripts",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Doors"
})

Tab:AddButton({
	Name = "Vynixius",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Vynixius/main/Doors/Script.lua"))()
  	end    
})

local Hubs = Window:MakeTab({
	Name = "Hubs",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Hubs:AddSection({
	Name = "My Scripts"
})

Hubs:AddButton({
	Name = "Blox Fruits",
	Callback = function()
      		loadstring(game:HttpGet("raw.githubusercontent.com/zDoggoOFC/e/main/BloxFruits"))()
  	end    
})

Hubs:AddButton({
	Name = "Anime Fighters",
	Callback = function()
      		loadstring(game:HttpGet("raw.githubusercontent.com/zDoggoOFC/e/main/AnimeFighters"))()
  	end    
})


OrionLib:Init()
end
OrionLib:Init()

function CorrectKeyNotification()
	OrionLib:MakeNotification({
		Name = "Correct Password!",
		Content = "You have entered the correct key!",
		Image = "rbxassetid://4483345998",
		Time = 3
	})
end

function IncorrectKeyNotification()
	OrionLib:MakeNotification({
		Name = "Incorrect Password!",
		Content = "You have entered the incorrect key!",
		Image = "rbxassetid://4483345998",
		Time = 3
	})
end

local Tab = Window:MakeTab({
	Name = "Password",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Password",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Check Password",
	Callback = function()
      	if _G.KeyInput == _G.Key then
		MakeScriptHub()
		CorrectKeyNotification()
		else
			IncorrectKeyNotification()
		  end
  	end
})

OrionLib:Init()
